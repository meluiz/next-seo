{
  "name": "@meluiz/next-seo",
  "version": "1.0.7",
  "description": "Simple SEO management to Next.js v13+",
  "main": "./index.js",
  "types": "./index.d.ts",
  "author": "meluiz",
  "license": "ISC",
  "keywords": [
    "next",
    "seo",
    "nextjs",
    "next-seo",
    "next seo"
  ],
  "tags": [
    "next",
    "seo",
    "nextjs",
    "next-seo",
    "next seo"
  ],
  "engines": {
    "node": ">=10"
  },
  "files": [
    "index.js",
    "index.d.ts"
  ],
  "scripts": {
    "build": "yarn tsup --dts-resolve",
    "commit": "git-cz",
    "prepare": "husky install",
    "postversion": "npm publish",
    "format": "prettier --write .",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "release:major": "standard-version --release-as major"
  },
  "repository": "https://github.com/meluiz/next-seo.git",
  "devDependencies": {
    "@commitlint/cli": "^17.4.0",
    "@commitlint/config-conventional": "^17.4.0",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "array-union": "^3.0.1",
    "cz-conventional-changelog": "3.3.0",
    "esbuild": "^0.16.14",
    "husky": "^8.0.0",
    "prettier": "^2.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "standard-version": "^9.5.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "private": false,
  "dependencies": {
    "tsup": "^6.5.0"
  }
}
