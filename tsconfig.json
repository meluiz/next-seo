{
  "compilerOptions": {
    "target": "es2015",
    "module": "commonjs",
    "jsx": "react-jsx",
    "allowJs": true,
    "declaration": true,
    "sourceMap": false,
    "outDir": "./",
    "rootDir": "lib",
    "removeComments": false,
    "strict": true,
    "esModuleInterop": true,
    "incremental": true,
    "tsBuildInfoFile": ".tsbuildinfo"
  },
  "include": ["lib"],
  "exclude": ["*.js"]
}
